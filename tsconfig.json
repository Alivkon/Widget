{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true                               /* Skip type checking all .d.ts files. */
    
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
